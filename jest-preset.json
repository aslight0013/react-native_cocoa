{
  "haste": {
    "defaultPlatform": "macos",
    "platforms": ["android", "ios", "native", "macos"],
    "providesModuleNodeModules": [
      "react-native-macos"
    ]
  },
  "moduleNameMapper": {
    "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp)$": "RelativeImageStub",
    "^React$": "<rootDir>/node_modules/react"
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/node_modules/react-native-macos/Libraries/react-native/",
    "<rootDir>/node_modules/react-native-macos/packager/"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!(jest-)?react-native|react-clone-referenced-element)"
  ],
  "setupFiles": [
    "<rootDir>/node_modules/react-native-macos/jest/setup.js"
  ],
  "testEnvironment": "node"
}
